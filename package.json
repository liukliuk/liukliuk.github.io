{
  "name": "liukliuk.github.io",
  "version": "0.0.0",
  "private": true,
  "description": "Luca Pianella's personal website",
  "main": "index.js",
  "scripts": {
    "cp": "npm run cp:lightbox2",
    "cp:lightbox2": "for x in css images js; do npm run cp:lightbox2:$x; done",
    "cp:lightbox2:css": "cp node_modules/lightbox2/dist/css/lightbox.min.css assets/lib/lightbox2/css",
    "cp:lightbox2:images": "cp node_modules/lightbox2/dist/images/* assets/lib/lightbox2/images",
    "cp:lightbox2:js": "cp node_modules/lightbox2/dist/js/lightbox-plus-jquery.min.* assets/lib/lightbox2/js",
    "lint_css": "stylelint src/styles/*css src/styles/*/*css",
    "postcss": "npm run lint_css && postcss --config postcss.json",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "stylelint": {
    "extends": "stylelint-config-standard"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liukliuk/liukliuk.github.io.git"
  },
  "keywords": [
    "art"
  ],
  "author": {
    "name": "Gianluca Casati",
    "url": "http://g14n.info"
  },
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/liukliuk/liukliuk.github.io/issues"
  },
  "homepage": "http://lucapianella.it",
  "dependencies": {
    "autoprefixer": "^6.5.0",
    "cssnano": "^3.8.0",
    "lightbox2": "^2.8.2",
    "normalize.css": "^5.0.0",
    "postcss-cli": "^2.6.0",
    "postcss-import": "^8.1.2",
    "precss": "^1.4.0",
    "stylelint": "^7.4.0",
    "stylelint-config-standard": "^13.0.2"
  }
}
